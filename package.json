{
  "name": "@meigetsuid/idgenerator",
  "version": "2.1.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/@types/index.d.ts",
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "eslint": "eslint --cache --ext .js,.ts .",
    "eslint:fix": "eslint --cache --ext .js,.ts . --fix",
    "ci": "run-p eslint test",
    "test": "jest",
    "build": "tsc",
    "postbuild": "node ./add-js-extension.js dist/",
    "build:release": "run-p build:release:*",
    "build:release:cjs": "tsc -p tsconfig.cjs.json",
    "build:release:esm": "tsc -p tsconfig.esm.json",
    "postbuild:release:esm": "node ./add-js-extension.js dist/esm/",
    "prepack": "run-s build:release",
    "postinstall": "run-s build:release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeigetsuID/PersonalIDGenerator.git"
  },
  "author": "Meigetsu",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://idportal.meigetsu.jp/contact"
  },
  "homepage": "https://idportal.meigetsu.jp/",
  "description": "Meigetsu ID API Server Personal ID Generator",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@meigetsusoft/hash": "^1.0.0-patch01",
    "async-lock": "^1.4.1",
    "nodeeasyfileio": "^1.0.5"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
